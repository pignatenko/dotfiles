{
  "@jsdoc": {
    "prefix": "@jsdoc",
    "body": [
      "/**",
      "  * $0 ",
      " */"
    ]
  },
  "@param": {
    "prefix": "@param",
    "body": [
      "@param ${1:name} ${0: describe}"
    ]
  },
  "@returns": {
    "prefix": "@returns",
    "body": [
      "@returns ${0}"
    ]
  }
}
